func deg(x) 
{   
    return x * 57.2958; 
}

func rad(x) 
{
    return x * 0.0174533; 
}

func abs(x)
{
    var y;
    if( x < 0)
        y = -x;
    else
        y = x;
    return y;
}

func min()
{
    if(len(args) == 0)
        return null;

    if(args[0] is list)
    {
        var L = args[0]
        if(len(L) == 0)
            return null

        var m = L[0];
        for(var a in L)
        {
            if(a < m)
                m = a;
        }
        return m;
    }

    var m = args[0];
    for(var a in args)
    {
        if(a < m)
            m = a;
    }
    return m;
}

func max(a, b)
{
