}

func linspace(m, M, N)
{
    if(m is null)
        m = 0
    if(M is null)
        M = 100
    if(N is null)
        N = 100
    
    if(m > M)
    {
        var tmp = m
        m = M
        M = tmp
    }

    if(N <= 0)
        return []

    var inc = (M - m) / N
    var ret = [0] * N
    for(var i = 0; i < N; i++)
    {
        ret[i] = i * inc + m
    }
    
    return ret
}