native libcalc
{
    num add(num, num);
    num sub(num, num);
    num mul(num, num);
    num divi(num, num);

    double add_double(double, double);
    double sub_double(double, double);
    double mul_double(double, double);
    double divi_double(double, double);
}

var a = add(4, 6);
var b = sub(4, 6);
var c = mul(4, 6);
var d = divi(4, 6);
println(a);
println(b);
println(c);
println(d);

var e = add_double(4, 6);
var f = sub_double(4, 6);
var g = mul_double(4, 6);
var h = divi_double(4, 6);
println(e);
println(f);
println(g);
println(h);