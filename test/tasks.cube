func task1()
{
    for(var i in 1:100)
    {
        print('[{}]'.format(i));
    }
}

func task2()
{
    var start = clock();
    for(var i = 400; i >= 0; --i)
    {
        print('({})'.format(i));
    }
    return clock() - start;
}   

func task3()
{
    for(var i = 0; i < 1000; ++i)
    {
        print('<{}>'.format(i));
    }
}

async task1();
async task2();

var elapsed = await task2;
println("\nElapsed: ", elapsed);

async task3();
wait(3);
abort task3;
