func pretty(f, s)
{
    println(s * 30)
    args = args.from(nargs)
    f(*args)
    println(s * 30)
}

func measure(f)
{
    args = args.from(nargs)
    var start = clock()
    f(*args)
    var elapsed = clock() - start
    println('Elapsed: ${elapsed}s')
}

@measure
@pretty('#')
@pretty('@')
@pretty('+')
func main(a, b)
{
    println('Hello world')
}

main(1, 200)