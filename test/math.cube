var pi = 3.14159265359;

func deg(x) 
{   
    return x * 57.2958; 
}

func rad(x) 
{
    return x * 0.0174533; 
}

func abs(x)
{
    if( x < 0)
        y = -x;
    else
        y = x;
    return y;
}

func min(a, b)
{
    if(a < b)
        return a;
    return b;
}

func max(a, b)
{
    if(a > b)
        return a;
    return b;
}

func sqrt(n)
{
    var x = n;
    var y = 1;
    var e = 1e-6;
    while(x - y > e)
    {
        x = (x + y)/2;
        y = n/x;
    }
    return x;
}

func sin(x)
{
    var y = (16*x*(pi - x))/(5*pi^2 - 4*x*(pi -x));
    return y;
}

func cos(x)
{
    var y = sqrt(1 - sin(x)^2);
    return y;
}

