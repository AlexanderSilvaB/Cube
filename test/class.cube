class Animal
{
    var name = null;

    func init(name)
    {
        this.name = name
    }

    func say()
    {
        if(name == null)
            println('I am an animal')
        else
            println('I am a ', name)
    }
}

class Dog : Animal
{
    func init()
    {
        super.init('dog')
    }
}

class Cat : Animal
{
    func init()
    {
        super.init('cat')
    }
}

if(__name__ == '__main__')
{
    var a = Animal()
    var b = Dog()
    var c = Cat()

    a.say()
    b.say()
    c.say()
}