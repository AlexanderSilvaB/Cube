class Animal
{
    var name = none;

    func init(name)
    {
        this.name = name;
        if(name == none)
            println('I am an animal');
        else
            println('I am a ', name);
    }
}

class Dog : Animal
{
    func init()
    {
        super.init('dog');
    }
}

class Cat : Animal
{
    func init()
    {
        super.init('cat');
    }
}

var a = Animal(none);
var b = Dog();
var c = Cat();