class Animal
{
    var name
    func init()
    {
        
    }

    func say()
    {
        if(name == null)
            println('I am an animal')
        else
            println('I am a ', name)
    }
}

class Dog : Animal
{
    var name = 'Dog'
    func init()
    {
        super.init()
    }
}

class Cat : Animal
{
    var name = 'Cat'
    func init()
    {
        super.init()
    }
}


// Eval
var a = Animal()
var b = Dog()
var c = Cat()

a.say()
b.say()
c.say()
